          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="Save Load for Update" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import com.fasterxml.jackson.databind.ObjectMapper;
import java.util.ArrayList;
import java.util.LinkedHashMap;
import java.util.Date;
import java.lang.Class;

log.info(&quot;Frank 1&quot;);

if ( ! prev.isResponseCodeOK() )
{
	log.info(&quot;Response code is bad, leaving&quot;); //ToDo - record a failure
	return;
}
log.info(&quot;Frank 2&quot;);

//
// Load the JSON resoponse into a java Map
//
String jsonString = prev.getResponseDataAsString();
log.info(&quot;Frank 2.1&quot;);
ObjectMapper mapper = new ObjectMapper();
log.info(&quot;Frank 2.2&quot;);
Map rootAsMap = mapper.readValue(jsonString, Map.class);

log.info(&quot;Frank 2.5&quot;);

LinkedHashMap list = rootAsMap.get(&quot;balance&quot;);

log.info(&quot;Frank 3&quot;);

if ( list == null || list.size() == 0 )
{
	log.info(&quot;No balance returned, leaving&quot;);
	return;
	
}


// Open a file to hold responses. These are later used to construct update requests.
f = new FileOutputStream(&quot;cfmResponses&quot;+ctx.getThreadNum(), false); 


p = new PrintStream(f); 

log.info(&quot;Frank 4&quot;);
p.print( vars.get(&quot;playerId&quot;) );

// Cycle through the userProfiles

	
// shorter, using the default it variable
String [ ] fields = new  String [] {&quot;playerId&quot;, &quot;balance&quot;};

log.info(&quot;Frank 5&quot;);
for ( field : fields )
{
	String fieldValue = list.get(field);
	log.info(&quot;Value of Field: &quot;+field+&quot;=&quot;+fieldValue);
	p.print( &quot;,&quot;+field+&quot;,&quot;+fieldValue );
}

	p.print(&quot;\n&quot;);
	p.flush();

log.info(&quot;Frank 6&quot;);

p.close();
f.close();</stringProp>
